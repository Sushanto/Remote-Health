			"contents": "package DoctorSideGui;\n\nimport java.io.*;\nimport java.net.*;\n\npublic class MainServer\n{\n	static ServerSocket serverSocket;\n	static Socket socket;\n	static BufferedReader bin,systemread;\n	static PrintWriter pwriter;\n	public static final int FILE_SIZE=6022386;\n\n	public static void main(String args[])\n	{\n		try\n		{\n			InetAddress addr=InetAddress.getByName(\"192.168.233.1\");\n			serverSocket=new ServerSocket(5000,1,addr);\n			System.out.println(\"Server Ready\");\n\n			systemread=new BufferedReader(new InputStreamReader(System.in));\n\n			String request=\"\";\n\n			while(!request.equals(\"Close Server\"))\n			{\n\n				socket=serverSocket.accept();\n\n				bin=new BufferedReader(new InputStreamReader(socket.getInputStream()));\n				pwriter=new PrintWriter(socket.getOutputStream(),true);\n\n				request=bin.readLine();\n				System.out.println(\"Request from client: \"+request);\n				if(request.equals(\"Send file\"))\n					sendfile();\n				else if(request.equals(\"Recieve file\"))\n					recievefile();\n				pwriter.close();\n				bin.close();\n				socket.close();\n			}\n\n			bin.close();\n			serverSocket.close();\n		}\n		catch(Exception e)\n		{\n			e.printStackTrace();\n		}\n	}\n\n	static void sendfile()\n	{\n		try\n		{\n			String confirm=\"File sending initiated\";\n			pwriter.println(confirm);\n			System.out.println(confirm);\n			String filename=bin.readLine();\n			File file=new File(filename);\n			if(file.isFile())\n			{\n				pwriter.println(\"RTS \"+String.valueOf(file.length()));\n				System.out.println(\"RTS : \"+filename);\n				FileInputStream fis=new FileInputStream(file);\n				BufferedInputStream bis=new BufferedInputStream(fis);\n\n				byte[] bytearray=new byte[(int)file.length()];\n\n				bis.read(bytearray,0,bytearray.length);\n				OutputStream os=socket.getOutputStream();\n				os.write(bytearray,0,bytearray.length);\n				os.flush();\n				os.close();\n				bis.close();\n				fis.close();\n				System.out.println(\"File sending complete. File name: \"+file);\n			}\n			else\n			{\n				pwriter.println(\"Error\");\n			}\n\n		}\n		catch(Exception e)\n		{\n			e.printStackTrace();\n		}\n	}\n\n	static void recievefile()\n	{\n		try\n		{\n			System.out.println(\"File recieving initiated\");\n			pwriter.println(\"File recieving initiated\");\n\n			String filename=bin.readLine();\n			String filesize=bin.readLine();\n			File file=new File(filename);\n			file.createNewFile();\n			pwriter.println(\"CTS\");\n			System.out.println(\"CTS : \"+filename);\n			FileOutputStream fos=new FileOutputStream(file);\n			BufferedOutputStream bos=new BufferedOutputStream(fos);\n\n			byte[] bytearray=new byte[FILE_SIZE];\n			//bin.close();\n			InputStream is=socket.getInputStream();\n\n			int current=0,byteread=0;\n\n			while(byteread>-1)\n			{\n				byteread=is.read(bytearray,current,bytearray.length-current);\n				if(byteread>=0)\n					current+=byteread;\n			}\n\n			bos.write(bytearray,0,current);\n			bos.flush();\n			System.out.println(\"File recieve complete. File name : \"+filename);\n			bos.close();\n			fos.close();\n			socket.close();\n			socket=serverSocket.accept();\n			pwriter=new PrintWriter(socket.getOutputStream(),true);\n			if(filesize.equals(String.valueOf(file.length())))\n			{\n				pwriter.println(\"ACK\");\n				System.out.println(\"ACK\");\n			}\n			else\n			{\n				pwriter.println(\"NAC\");\n				System.out.println(\"NAC\");\n			}\n			is.close();\n		}\n		catch(Exception e)\n		{\n			e.printStackTrace();\n		}\n	}\n}\n",
