onPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_SelectKioskActionPerformed\n        try {\n            // select \n\n            put_values();\n        } catch (Exception ex) {\n            Logger.getLogger(Patient_list.class.getName()).log(Level.SEVERE, null, ex);\n        }\n    }//GEN-LAST:event_SelectKioskActionPerformed\n\n    /**\n     * @param args the command line arguments\n     */\n   \n    // Variables declaration - do not modify//GEN-BEGIN:variables\n    private javax.swing.JLabel DocotorName;\n    private javax.swing.JLabel DoctorRegistrationNumber;\n    private javax.swing.JButton Logout;\n    private javax.swing.JButton SelectKiosk;\n    private javax.swing.JComboBox SelectKioskComboBox;\n    private javax.swing.JButton SelectPatient;\n    private javax.swing.JComboBox SelectPatientComboBox;\n    private javax.swing.JLabel jLabel1;\n    private javax.swing.JLabel jLabel2;\n    private javax.swing.JLabel jLabel5;\n    private javax.swing.JLabel jLabel6;\n    private javax.swing.JLabel jLabel7;\n    private javax.swing.JPanel jPanel1;\n    private javax.swing.JPanel jPanel2;\n    // End of variables declaration//GEN-END:variables\n    \n    \n    /*\n    fill values of the comboboxes by getting the log file from the server computer \n    stores them in a arraylist and puts them in the select patient combobox ....\n    \n    */\n    void put_values()throws Exception{\n        kiosk_num=SelectKioskComboBox.getSelectedItem().toString();\n        Thread.sleep(100);\n        if(connection.receiveFromServer(\"Server/PatientInfo/Patient_\"+kiosk_num+\"_Log.xml\",relative_path+log)==false){\n            jLabel7.setText(\"connection error!!\");\n        }else{\n        \n        JAXBContext context;\n        jLabel7.setText(\"\");\n        try {\n            context = JAXBContext.newInstance(PatientLog.class);\n            Unmarshaller um = context.createUnmarshaller();\n           pl = (PatientLog) um.unmarshal(new File(relative_path+log));\n            \n            \n            \n        } catch (JAXBException ex) {\n            Logger.getLogger(Patient_Report.class.getName()).log(Level.SEVERE, null, ex);\n            \n        }\n                    \n                    ArrayList<String > as= pl.getnormal();\n                    String s;\n                    \n                     DefaultComboBoxModel model = (DefaultComboBoxModel) SelectPatientComboBox.getModel();\n                     model.removeAllElements();\n                     int siz=as.size()-1;\n                     while(siz>=0){\n                         model.addElement(as.get(siz));\n                         siz--;   \n                     }\n                     as=pl.getemergency();\n                     siz=as.size()-1;\n                     while(siz>=0){\n                         model.addElement(as.get(siz)+\" E\");\n                         siz--;\n                         \n                     }\n                     SelectPatientComboBox.setModel(model);\n                     \n                     \n        }\n        \n    }\n    \n     /*\n        this puts doctor name , registration number etc values ... you can add more ,,,\n    \n    */\n		\n    void put_primary_values(){\n                     DocotorName.setText(DocotorName.getText()+Name);\n      \n                     DoctorRegistrationNumber.setText(DoctorRegistrationNumber.getText()+Doc_Reg_number);\n    }\n    \n}\n",
