String();\n            try {\n                Doctor_count dc=new Doctor_count();\n                id = dc.returncount();\n            } catch (FileNotFoundException ex) {\n                Logger.getLogger(sign_up.class.getName()).log(Level.SEVERE, null, ex);\n            } catch (IOException ex) {\n                Logger.getLogger(sign_up.class.getName()).log(Level.SEVERE, null, ex);\n            }\n        doc_id=\"doc\";\n        doc_id=doc_id+id;\n        jTextField2.setText(doc_id);\n        d.doctor_id = doc_id;                   \n        try  {\n		JAXBContext jaxbContext = JAXBContext.newInstance(Doctor_xml.class);\n		Marshaller jaxbMarshaller = jaxbContext.createMarshaller();\n		jaxbMarshaller.setProperty(Marshaller.JAXB_FORMATTED_OUTPUT, true);\n		jaxbMarshaller.marshal(d, new File(\"C:\\\\Users\\\\debd\\\\Desktop\\\\server\\\\doctor\\\\doc.xml\"));\n		//jaxbMarshaller.marshal(d, System.out);\n	     } catch (JAXBException e) {\n		System.out.println(\"Error\");\n	     }  \n        JOptionPane.showMessageDialog(this,\"Data saved successfully.\\nDoctor id is created and \"\n            +\"Doctor id is \"+doc_id);\n        //cl.SendToServer(\"C:\\\\Users\\\\debd\\\\Desktop\\\\server\\\\doctor\\\\doc.xml\",\"Server/doctor/doc\"+id+\".xml\");\n        File f=new File(\"C:\\\\Users\\\\debd\\\\Desktop\\\\server\\\\doctor\\\\doc.xml\");\n        /*boolean p=f.delete();\n        if(p){\n            System.out.println(\"doctor fiile deleted and send to server\\n\");\n        }else{\n            System.out.println(\"nooo doctor fiile not deleted\\n\");\n        }\n        * */\n     }\n        \n    }//GEN-LAST:event_jButton2ActionPerformed\n\n    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed\n        File f=new File(\"C:\\\\Users\\\\debd\\\\Desktop\\\\server\\\\doctor\\\\log.txt\");\n        boolean b=f.delete();\n        if(b){\n            System.out.println(\"doctor log fiile deleted and send to server\\n\");\n        }else{\n            System.out.println(\"nooo log doctor fiile not deleted\\n\");\n        }\n        DoctorLoginFrom back =new DoctorLoginFrom();\n        back.setVisible(true);\n        \n        super.dispose();\n    }//GEN-LAST:event_jButton5ActionPerformed\n\n    private void jTextField1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField1ActionPerformed\n       \n    }//GEN-LAST:event_jTextField1ActionPerformed\n\n    private void jTextField15ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField15ActionPerformed\n       \n    }//GEN-LAST:event_jTextField15ActionPerformed\n\n    private void jTextField12ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField12ActionPerformed\n       \n    }//GEN-LAST:event_jTextField12ActionPerformed\n\n    private void jTextField13ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField13ActionPerformed\n     \n    }//GEN-LAST:event_jTextField13ActionPerformed\n\n    private void jTextField14ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField14ActionPerformed\n      \n    }//GEN-LAST:event_jTextField14ActionPerformed\n\n    private void jTextField16ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField16ActionPerformed\n        \n    }//GEN-LAST:event_jTextField16ActionPerformed\n\n    private void jTextField17ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField17ActionPerformed\n       \n    }//GEN-LAST:event_jTextField17ActionPerformed\n\n    private void jTextField3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField3ActionPerformed\n       \n    }//GEN-LAST:event_jTextField3ActionPerformed\n\n    private void jTextField5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jTextField5ActionPerformed\n      \n    }//GEN-LAST:event_jTextField5ActionPerformed\n\n    private void jTextField6ActionPerformed(