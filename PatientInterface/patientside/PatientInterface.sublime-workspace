       try {\n            JAXBContext context;\n            context = JAXBContext.newInstance( Patient_Report.class);\n            Marshaller m = context.createMarshaller();\n            m.setProperty(Marshaller.JAXB_FORMATTED_OUTPUT, true);\n            //m.marshal(pr, new File(\"C:\\\\Users\\\\debd\\\\Desktop\\\\patient\\\\pat.xml\"));\n             m.marshal(pr, new File(relative_path+path));\n        } catch (JAXBException ex) {\n            Logger.getLogger(Doctor_interface.class.getName()).log(Level.SEVERE, null, ex);\n        }\n        \n        cl.SendToServer(relative_path+path,\"Server/PatientInfo/\"+Patient_id+\".xml\");\n        File f=new File(relative_path+path);\n        f.delete();\n        \n        String st=this.Patient_id;\n\n        JAXBContext context;\n\n        try {\n            context = JAXBContext.newInstance(PatientLog.class);\n            Unmarshaller um = context.createUnmarshaller();\n            pl = (PatientLog) um.unmarshal(new File(relative_path+Log_path));\n\n        } catch (JAXBException ex) {\n            Logger.getLogger(Patient_Report.class.getName()).log(Level.SEVERE, null, ex);\n\n        }\n        ArrayList<String> ase=null;\n        if(pl.getemergency()!=null&&pl.getemergency().size()>0){\n            ase=pl.getemergency();\n            if(ase.indexOf(st)!=-1)\n            ase.remove(ase.indexOf(st));\n        }\n        \n        \n        ArrayList<String> as=null;\n        if(pl.getnormal()!=null&&pl.getnormal().size()>0){\n             as=pl.getnormal();\n             if(as.indexOf(st)!=-1)\n             as.remove(as.indexOf(st));\n        }\n           \n        \n        pl.setemergency(ase);\n        \n        pl.setnormal(as);\n        try {\n\n            context = JAXBContext.newInstance( PatientLog.class);\n            Marshaller m = context.createMarshaller();\n            m.setProperty(Marshaller.JAXB_FORMATTED_OUTPUT, true);\n            m.marshal(pl, new File(relative_path+Log_path));\n\n        } catch (JAXBException ex) {\n            Logger.getLogger(PatientLog.class.getName()).log(Level.SEVERE, null, ex);\n        }\n        \n        cl.SendToServer(relative_path+Log_path,\"Server/PatientInfo/Patient_\"+kiosk_num+\"_Log.xml\");\n        \n        File ff=new File(relative_path+Log_path);\n        ff.delete();\n        File file;\n         for(int i=0;i<list_of_files.length;i++){\n                        file=new File(list_of_files[i]);\n                        file.delete();\n                         \n         }\n        //(new File(\"C:\\\\Users\\\\debd\\\\Desktop\\\\patient\\\\log.xml\")).delete();\n\n        new Patient_list(reg_number,name,signature).setVisible(true);\n       // this.patient_list.setVisible(true);\n        super.dispose();\n\n        }\n    }//GEN-LAST:event_submit_buttonActionPerformed\n\n    private void back_buttonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_back_buttonActionPerformed\n        // TODO add your handling code here:\n        File f=new File(relative_path+path);\n        f.delete();\n         File file;\n        for(int i=0;i<list_of_files.length;i++){\n                        file=new File(list_of_files[i]);\n                        file.delete();\n                         \n         }\n        new Patient_list(reg_number,name,signature).setVisible(true);\n       // this.patient_list.setVisible(true);\n        super.dispose();\n    }//GEN-LAST:event_back_buttonActionPerformed\n\n    private void reset_buttonActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_reset_buttonActionPerformed\n        // TODO add your handling code here:\n        diagnostic_test_textarea.setText(null);\n        medication_textarea.setText(null);\n        referral.setText(null);\n        advice_textarea.setText(null);\n\n    }//GEN-LAST:event_reset_buttonActionPerformed\n\n    private void gender_textfiedActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_gender_textfiedActionPerformed\n        